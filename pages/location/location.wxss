.logs{
    width: 100%;
    height: 300rpx;
    text-align: center;
    padding: 20rpx;
    box-shadow: border-box;
    overflow: hidden;
    border: 1px #cccccc solid;
    margin: 20rpx 0;
}
.logs scroll-view{
    height: 100%;
    overflow: scroll;
}
scroll-view view{
    margin: 10rpx;
}
.badge{
    display: inline-block;
    width: 10rpx;
    height: 10rpx;
    vertical-align: middle;
}
.red{
    background: #d81e06;
}
.yellow{
    background: #f4ea2a; 
}
.green{
    background: #1afa29; 
}
button{
    float: left;
    margin: 30rpx;
    color: #ffffff;
}